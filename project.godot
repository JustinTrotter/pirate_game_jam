; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Pirate Game Jam"
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
run/max_fps=120
config/icon="res://icon.svg"

[aseprite_importers]

aseprite_executable_path="/usr/bin/aseprite"

[autoload]

Globals="*res://globals/globals.tscn"
Utils="*res://globals/utils.tscn"
SoundManager="*res://addons/sound_manager/sound_manager.gd"
AssetCache="*res://globals/asset_cache.tscn"

[display]

window/size/viewport_width=960
window/size/viewport_height=608
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/amano-ldtk-importer/plugin.cfg", "res://addons/nklbdev.aseprite_importers/plugin.cfg", "res://addons/sound_manager/plugin.cfg")

[importer_defaults]

LDtk_world.import={
"Entities": "",
"Levels": "",
"Tilesets": "",
"entity_add_metadata": true,
"entity_post_import_script": "res://addons/amano-ldtk-importer/examples/post-import-scripts/post-import-entities-create-nodes.gd",
"import_all_levels": true,
"import_entities": true,
"import_tileset_custom_data": true,
"level_add_metadata": true,
"level_post_import_script": "",
"levels_to_import": "0,1",
"pack_levels": false,
"tileset_add_metadata": true,
"tileset_post_import_script": "res://addons/amano-ldtk-importer/examples/post-import-scripts/post-import-tileset-add-collisions-by-enum.gd",
"world_add_metadata": false,
"world_post_import_script": ""
}

